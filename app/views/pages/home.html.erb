  <div class="banner-call">
    <div class="banner-question-category">
      <div class="container">
        <h1 id="home-title" class="rellax" data-rellax-speed="4">ToGather</h1>
      </div>
      <div class="container">
        <div class="banner-question">
          <button class="home-init-button js-home-init-btn rellax" data-rellax-speed="1">Lets get togather</button>
        </div>
      </div>
    </div>
  </div>
  <div class="category-form-container">
  <!--   <div class="filler-div"></div> -->
    <%= simple_form_for @activity, method: :get do |f| %>
      <div class="banner-question-category js-home-category-question d-flex justify-content-center align-items-center">
        <div class="question-category-container">
          <div id="banner-question-category-header">
            <h1>What are you in the mood for?</h1>
          </div>
          <div class="category-buttons">
            <%= f.input :category, as: :radio_buttons, collection: ['Sweat', 'Romance', 'Outdoorsy', 'Low-fi', 'Turnt', 'Broke-ass'], :item_wrapper_class => 'inline', label: false %>
          </div>
        </div>
      </div>
      <div class="banner-question-date js-home-date-question d-flex flex-column justify-content-center align-items-center">
        <h1 id="banner-question-date-header">Which date do you fancy?</h1>
        <div class="container d-flex justify-content-center">
          <div class="date-buttons">
            <%# f.date_field :start_time %>
            <%= f.input :start_time, as: :date, label: false, class: "text-right" %>
            <%= f.submit "Find me something to do!", class: "btn btn-primary", id: "form-search-button" %>
            <div id="autotype-day"><p></p></div>
            <div id="autotype-month"><p></p></div>
            <div id="autotype-year"><p></p></div>
            <div id="arrow-day-date" style="background-image: url(https://cdn4.iconfinder.com/data/icons/hand-drawn-arrows/100/hand_drawn_arrows-18-512.png)"></div>
            <div id="arrow-day-month" style="background-image: url(https://cdn4.iconfinder.com/data/icons/hand-drawn-arrows/100/hand_drawn_arrows-18-512.png)"></div>
            <div id="arrow-day-year" style="background-image: url(https://cdn4.iconfinder.com/data/icons/hand-drawn-arrows/100/hand_drawn_arrows-18-512.png)"></div>
          </div>
        </div>
        <div class="tree-image"></div>
        <div class="sun-image"></div>
      </div>
    <% end %>

  </div>
