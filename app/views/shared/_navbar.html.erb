<div class="d-flex justify-content-center navbar navbar-expand-sm navbar-light navbar-logo">
  <div>
    <%= link_to root_path, class: "navbar-brand" do %>
      <%= image_tag('3.png') %>
    <% end %>
  </div>

  <div class="middle-nav">
    <%= link_to "Hello", dashboard_path, class: "nav-link" %>
    <%= link_to "Sick Vibes", dashboard_path, class: "nav-link" %>
    <%= link_to "Party at Fili's", dashboard_path, class: "nav-link" %>
  </div>

  <div class="d-flex justify-content collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav">
      <% if user_signed_in? %>

          <%= link_to "Dashboard", dashboard_path, class: "nav-link" %>
          <%= link_to "Messages", dashboard_path, class: "nav-link" %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "nav-link" %>
      <% else %>
        <%= link_to "Login", new_user_session_path, class: "nav-link" %>
      <% end %>
    </ul>
  </div>
</div>
